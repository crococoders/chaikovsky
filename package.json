{
  "name": "tchaikovsky",
  "productName": "tchaikovsky",
  "version": "0.1.0",
  "description": "Cross-platform Yandex Music desktop application",
  "main": "./.webpack/main",
  "keywords": [],
  "author": {
    "name": "abdigali",
    "email": "abdigali2@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "electron-forge start",
    "start:server": "sudo docker run -it -p 8080:8000 nartayzhanybekov/tchaikovsky",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts . --color",
    "test": "jest",
    "test:snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cucumber": "cucumber-js",
    "check-types": "tsc",
    "storybook": "start-storybook"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "template_ts_webp"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./static/index.html",
                  "js": "./app/renderer/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.50",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@storybook/react": "^5.3.19",
    "@types/cucumber": "^6.0.1",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "electron": "11.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-loader": "^1.1.0",
    "husky": "^4.2.5",
    "install": "^0.13.0",
    "jest": "^25.3.0",
    "jest-cli": "^25.3.0",
    "jsdom": "16.2.2",
    "jsdom-global": "3.0.2",
    "lint-staged": ">=10",
    "mocha": "^7.1.1",
    "node-fetch": "^2.6.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "npm": "^6.14.6",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^9.0.1",
    "spectron": "^10.0.1",
    "style-loader": "^0.23.1",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/chai": "^4.2.11",
    "@types/enzyme": "^3.10.5",
    "@types/mocha": "^7.0.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-test-renderer": "^16.9.2",
    "and": "0.0.3",
    "chromedriver": "^81.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "gherkin": "^9.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "sass-resources-loader": "^2.0.3",
    "selenium-webdriver": "^4.0.0-alpha.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test && npm run check-types",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-length": [
        1,
        "always",
        85
      ],
      "scope-empty": [
        2,
        "always"
      ]
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  }
}